{% block javascripts %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-datetimepicker/2.5.16/jquery.datetimepicker.full.min.js"
            integrity="sha384-3855a0df405d3243aba405371dd23d56d9e9e0b9d01299a9ca73b0c8b9c8fc8067000155458d045b6c3611b6edacac77">
    </script>

    <!-- Default increment size is 15min. To change, set step to custom amount -->
    <script>
        const options = {
            format: 'd.m.Y H:i',
            lang: 'no',
            step: {{ step|default(15) }},
            dayOfWeekStart: 1,
        };
        jQuery.datetimepicker.setLocale('no');
    {% for input in inputIds %}
    jQuery('#{{input}}').datetimepicker(options);
    {% endfor %}
    </script>
{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet"
          href="https://cdnjs.cloudflare.com/ajax/libs/jquery-datetimepicker/2.5.16/jquery.datetimepicker.min.css">
{% endblock %}
